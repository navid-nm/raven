use vscode

|| diagnostics  -> DiagnosticCollection
|| sfile        -> File
class RavenLinter {
   init {
      my.diagnostics = vscode.languages.createDiagnosticCollection("raven");
   }

   lintDocument(sfile) {
      if (sfile.languageId != "raven") {
         return;
      }

      let diagnostics = [];
      // val text = sfile.getText();
      // val lines = text.split("\n");
      my.diagnostics.set(sfile.uri, diagnostics);
   }

   dispose() {
      my.diagnostics.dispose();
   }
}
